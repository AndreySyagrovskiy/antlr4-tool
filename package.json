    "commander": "^2.16.0",
    "directory-exists": "^2.0.0",
    "ejs": "^2.6.1",
    "lodash": "^4.17.10",
    "mkdir-recursive": "^0.4.0",
    "node-eval": "^2.0.0",
    "targz": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcchatman8009/antlr4-tool"
